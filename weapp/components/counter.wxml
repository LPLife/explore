
<view class="counter {{style}}">
  <button size="mini" data-wpy-evt="17-0" bindtap="_proxy">  +  </button>
  <button size="mini" data-wpy-evt="17-1" bindtap="_proxy">  -  </button>
  <button size="mini" data-wpy-evt="17-2" bindtap="_proxy">  INCREMENT  </button>
  <button size="mini" data-wpy-evt="17-3" bindtap="_proxy">  DECREMENT  </button>
  <button size="mini" data-wpy-evt="17-4" bindtap="_proxy">  ASYNC INCREMENT  </button>
  <label class="count"> {{counter}} </label>
</view>
